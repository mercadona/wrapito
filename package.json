{
  "name": "wrapito",
  "version": "0.0.0",
  "description": "ðŸŒ¯ ðŸŒ¯ Wrap you tests so that you can test both behaviour and components with less effort.",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --ci --coverage --config config/jest.config.json",
    "test:watch": "jest --watch --config config/jest.config.json",
    "build": "NODE_ENV=production tsc",
    "clean": "rm -rf dist",
    "postbuild": "node scripts/copy-publish-files.js",
    "publish-package": "npm run clean && npm run build && npm run postbuild && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mercadona/wrapito.git"
  },
  "keywords": [
    "react",
    "testing",
    "test",
    "integration",
    "unit",
    "javascript",
    "dom",
    "js",
    "jest",
    "wrap",
    "component",
    "mock",
    "fetch",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/mercadona/wrapito/issues"
  },
  "homepage": "https://github.com/mercadona/wrapito#readme",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "object-hash": "^2.1.1",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.16",
    "axios": "^1.2.2",
    "babel-jest": "^24.8.0",
    "jest": "^27.0.4",
    "jest-junit": "^6.4.0",
    "react": "^17.0.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.3"
  },
  "peerDependencies": {
    "jest": "*",
    "chalk": "*",
    "react": "*",
    "react-dom": "*"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/proposal-class-properties"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "results/tests/jest-junit.xml"
  }
}
