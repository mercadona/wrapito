{
  "name": "@mercadona/mo.library.burrito",
  "version": "0.0.0",
  "description": "ðŸŒ¯ ðŸŒ¯ Wrap you tests so that you can test both behaviour and components with less effort.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --ci --coverage --config config/jest.config.json",
    "test:watch": "jest --watch --config config/jest.config.json",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps",
    "clean": "rm -rf dist",
    "postbuild": "node scripts/copy-publish-files.js",
    "publish-package": "npm run clean && npm run build && npm run postbuild && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mercadona/mo.library.burrito.git"
  },
  "keywords": [
    "react",
    "testing",
    "test",
    "integration",
    "unit",
    "javascript",
    "dom",
    "js",
    "jest",
    "wrap",
    "burrito",
    "component",
    "mock",
    "fetch",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/mercadona/mo.library.burrito/issues"
  },
  "homepage": "https://github.com/mercadona/mo.library.burrito#readme",
  "dependencies": {
    "deep-equal": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.1.0",
    "jest-junit": "^6.4.0",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "peerDependencies": {
    "jest": "*",
    "chalk": "*",
    "react": "*",
    "react-dom": "*"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/proposal-class-properties"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "results/tests/jest-junit.xml"
  }
}